convert store/icon.png -geometry 48x48 app/src/main/res/mipmap-mdpi/ic_launcher.png
convert store/icon.png -geometry 72x72 app/src/main/res/mipmap-hdpi/ic_launcher.png
convert store/icon.png -geometry 96x96 app/src/main/res/mipmap-xhdpi/ic_launcher.png
convert store/icon.png -geometry 144x144 app/src/main/res/mipmap-xxhdpi/ic_launcher.png
convert store/icon.png -geometry 192x192 app/src/main/res/mipmap-xxxhdpi/ic_launcher.png

convert store/icon.png -geometry 48x48 app/src/main/res/mipmap-mdpi/ic_launcher_round.png
convert store/icon.png -geometry 72x72 app/src/main/res/mipmap-hdpi/ic_launcher_round.png
convert store/icon.png -geometry 96x96 app/src/main/res/mipmap-xhdpi/ic_launcher_round.png
convert store/icon.png -geometry 144x144 app/src/main/res/mipmap-xxhdpi/ic_launcher_round.png
convert store/icon.png -geometry 192x192 app/src/main/res/mipmap-xxxhdpi/ic_launcher_round.png

